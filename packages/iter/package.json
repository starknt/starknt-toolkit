{
  "name": "@starknt/iter",
  "type": "module",
  "version": "0.0.5",
  "private": false,
  "description": "TypeScript iterator library inspired by Rust's Iterator trait",
  "author": "starknt",
  "license": "MIT",
  "keywords": [
    "iterator",
    "rust",
    "iter",
    "functional",
    "typescript",
    "lazy"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./globals": {
      "types": "./dist/globals.d.ts",
      "import": "./dist/globals.js",
      "require": "./dist/globals.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublish": "pnpm build",
    "test": "vitest --run",
    "lint:fix": "eslint {src,tsdown.config.ts} --fix"
  },
  "dependencies": {
    "@starknt/utils": "workspace:*"
  },
  "devDependencies": {
    "tsdown": "^0.19.0-beta.2"
  }
}
